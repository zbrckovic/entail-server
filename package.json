  "dependencies": {
    "@zbrckovic/entail-core": "0.0.45",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "standard": "^16.0.2"
  },
  "nodemonConfig": {
    "restartable": "rs",
